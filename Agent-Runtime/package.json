{
  "name": "agent-runtime",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch src/server.ts",
    "build": "tsc && esbuild src/server.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/server.js",
    "check": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "db:push": "drizzle-kit generate && drizzle-kit migrate"
  },
  "dependencies": {
    "@livekit/agents": "^1.0.20",
    "livekit-server-sdk": "^2.14.2",
    "drizzle-orm": "^0.44.5",
    "express": "^4.21.2",
    "nanoid": "^5.1.5",
    "postgres": "^3.4.5",
    "zod": "^4.1.12",
    "dotenv": "^17.2.2"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "^24.7.0",
    "drizzle-kit": "^0.31.4",
    "esbuild": "^0.25.0",
    "prettier": "^3.6.2",
    "tsx": "^4.19.1",
    "typescript": "5.9.3",
    "vitest": "^2.1.4"
  }
}

